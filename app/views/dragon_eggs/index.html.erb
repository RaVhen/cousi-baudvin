<h1>Listing dragon_eggs</h1>

<table>
  <thead>
    <tr>
      <th>Race</th>
      <th>Count</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dragon_eggs.each do |dragon_egg| %>
      <tr>
        <td><%= dragon_egg.race %></td>
        <td><%= dragon_egg.count %></td>
        <td><%= dragon_egg.description %></td>
        <td><%= link_to 'Show', dragon_egg %></td>
        <td><%= link_to 'Edit', edit_dragon_egg_path(dragon_egg) %></td>
        <td><%= link_to 'Destroy', dragon_egg, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dragon egg', new_dragon_egg_path %>
